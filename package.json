  "dependencies": {
    "@dannymoerkerke/fido2-lib": "^2.4.1",
    "@dannymoerkerke/material-webcomponents": "^1.2.14",
    "base64url": "^3.0.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "error-handler": "^1.0.0",
    "errorhandler": "^1.5.1",
